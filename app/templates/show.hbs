<div class="row">
  <div class="col s2">
    <h3 role="button" class="btn-large btn-flat" {{action "goBack"}}>
      <MaterialIcon @name="arrow_back" />
    </h3>
  </div>

  <div class="col s8 center-align">
    <h3>Contact list</h3>
  </div>
</div>

<div class="row">
  <div class="col s12">
    <div class="card horizontal hoverable">
      <div class="card-image">
        <img src={{model.contact.avatar}} alt="public/user.png">
      </div>
      <div class="card-stacked">
        <div class="card-content">
          <h5>{{model.contact.first_name}} {{model.contact.last_name}}</h5>
          <span>{{model.contact.email}}</span>
        </div>
      </div>
    </div>
  </div>
</div>


